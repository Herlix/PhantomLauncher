<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" snapToPixel="true" visible="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="phantomlauncher.Login">
  <children>
    <ImageView id="test.jpg" layoutX="0.0" layoutY="0.0">
      <image>
        <Image url="@test.jpg" preserveRatio="true" smooth="true" />
      </image>
    </ImageView>
    <Label layoutX="187.0" layoutY="92.0" text="Password:" textFill="WHITE">
      <labelFor>
        <PasswordField id="" fx:id="passID" alignment="CENTER" layoutX="187.0" layoutY="107.0" prefHeight="32.0" prefWidth="220.0" promptText="Password" text="" />
      </labelFor>
    </Label>
    <Label layoutX="187.0" layoutY="44.0" text="Username:" textFill="WHITE">
      <labelFor>
        <TextField fx:id="idUser" alignment="CENTER" layoutX="187.0" layoutY="60.0" opacity="1.0" prefHeight="32.0" prefWidth="220.0" promptText="Username" text="" />
      </labelFor>
    </Label>
    <Text fx:id="error" fill="RED" layoutX="214.0" layoutY="41.0" strikethrough="false" strokeType="OUTSIDE" strokeWidth="0.0" text="" textAlignment="CENTER" wrappingWidth="0.0">
      <font>
        <Font size="12.0" />
      </font>
    </Text>
    <fx:reference source="idUser" />
    <fx:reference source="passID" />
    <Button fx:id="login" layoutX="357.0" layoutY="148.0" onAction="#loginAction" text="Login">
      <effect>
        <DropShadow height="17.0" radius="8.0" width="17.0" />
      </effect>
    </Button>
    <Button fx:id="signUp" layoutX="187.0" layoutY="148.0" mnemonicParsing="false" onAction="#signUpAction" text="Signup">
      <effect>
        <DropShadow height="17.0" radius="8.0" width="17.0" />
      </effect>
    </Button>
    <Button fx:id="closeDown" layoutX="564.0" layoutY="8.0" minHeight="15.9609375" minWidth="21.0" mnemonicParsing="false" onAction="#closeDown" prefHeight="22.0" prefWidth="22.0" text="X">
      <effect>
        <DropShadow height="17.0" radius="8.0" width="17.0" />
      </effect>
      <font>
        <Font size="9.0" />
      </font>
    </Button>
    <Button fx:id="minimize" layoutX="535.0" layoutY="8.0" minHeight="15.9609375" minWidth="21.0" mnemonicParsing="false" onAction="#minimize" prefHeight="22.0" prefWidth="22.0" text="-" textAlignment="CENTER">
      <effect>
        <DropShadow height="17.0" radius="8.0" width="17.0" />
      </effect>
      <font>
        <Font name="System Bold" size="10.0" />
      </font>
    </Button>
  </children>
  <effect>
    <ColorAdjust />
  </effect>
</AnchorPane>
